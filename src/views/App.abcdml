App:
  <<: 
    onnotice: callback = EMPTY
  {}:
    name: @state1
  >>:
    error: ->Log.Error(@event, AppError)
    notice: $onnotice
  []: 
    - RoutesLayer
        - Route: 
            path: /home
            name: home
            com: HomePage
        - Route: 
            path: /404
            name: notFound
            com； NotFound
        - DefaultRoute:
            route: home
            # 在本示例中，上述配置等价于下面的配置
